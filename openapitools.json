{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.13.0",
    "generators": {
      "horizont-system": {
        "generatorName": "typescript-angular",
        "output": "./src/app/rest",
        "inputSpec": "./openapi/openapi.json",
        "additionalProperties": {
          "ngVersion": "19.0.0",
          "supportsES6": true,
          "fileNaming": "kebab-case",
          "apiModulePrefix": "Horizont",
          "modelSuffix": "DTO",
          "serviceSuffix": "HorizontService",
          "useSquareBracketsInArrayNames": true
        },
        "typeMappings": {
          "set": "Array"
        }
      }
    }
  }
}
